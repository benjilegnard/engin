<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    {% set basePath = '/maquettes' %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Etre-Marin.fr : index des maquettes HTML</title>
    <meta name="description" content="index des maquettes HTML">
    <meta name="viewport" content="width=device-width">
    <meta http-equiv="Cache-control" content="public" />

    <link rel="stylesheet" type="text/css" href="{{ basePath }}/css/bootstrap.css">

    <link rel="stylesheet" href="{{ basePath }}/css/em.css">
    <link rel="stylesheet" href="{{ basePath }}/css/mq.css">
    <link rel="stylesheet" href="{{ basePath }}/css/sprites/em-fonds-sprite.css">

    <link rel="stylesheet" href="{{ basePath }}/css/sprites/atc-icons-sprite.css">

    {% block head_css%}{% endblock %}

    {# Compatibilité Internet Explorer < 9 #}
    <!--[if lt IE 9]>
    <link rel="stylesheet" href="{{ basePath }}/css/ie.css">
    <![endif]-->

    {# FAVICONES #}

    <link rel="shortcut icon"       href="{{ basePath }}/favicon.ico">
    {# iOS 1.1.3+: this is the icon that's used when the user adds your app to the home screen, also works on Android! #}
    <link rel="apple-touch-icon" href="icon.png"/>

    {# iOS 2.0+: tell iOS not to apply any glare effects to the icon #}
    <link rel="apple-touch-icon-precomposed" href="icon.png"/>

    {# iOS 4.2+ icons for different resolutions #}
    <link rel="apple-touch-icon" sizes="72x72" href="{{ basePath }}/ico/apple-touch-icon-72x72-precomposed.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="{{ basePath }}/ico/apple-touch-icon-144x144-precomposed.png" />

    {# iOS 3+: full-screen startup splash screen image (must be 320x460)
    <link rel="apple-touch-startup-image" href="startup.png">   #}

    {# enable full-screen mode (only when launched from home screen) #}
    <meta name="apple-mobile-web-app-capable" content="yes" />

    {# Contrôle l'apparence de la barre de statut en plein écran. #}
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />

    <link rel="apple-touch-icon"    href="{{ basePath }}/ico/apple-touch-icon.png">

    <link rel="stylesheet" href="{{ basePath }}/css/bootstrap.css">
    <link rel="stylesheet" href="{{ basePath }}/css/mq.css">
    <link rel="stylesheet" href="{{ basePath }}/css/atc-engine.css">

    <script data-main="js/engine" src="js/libs/require.js"></script>

</head>
<body class="atc-orange atc-engine">
{% block header %}

<ul class="atc-nav atc-head">
    <li class="atc-handle">
        <a class="atc-icon-color">ATC</a>
    </li>
    <li class="dropdown">
        <a href="{{ basePath }}/index.html"
           class="dropdown-toggle" role="button"
           data-toggle="dropdown" data-target="parent">Pages<span class="caret"></span>
        </a>
        <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
            {% for layout in layouts.gabarits %}
            <li><a href="/maquettes/{{ layout.key }}.html">{{ layout.title }}</a></li>
            {% endfor %}

            <li class="divider"></li>
            <li><a href="/maquettes/espace-candidat.html">Espace RH</a></li>

            <li><a href="/maquettes/maintenance.html">maintenance</a></li>
            <li><a href="/maquettes/erreur.html">erreur</a></li>
            <li class="dropdown-submenu">
                <a tabindex="-1" href="#">More options</a>
                <ul class="dropdown-menu">
                    <li><a tabindex="-1" href="#">Another action</a></li>
                    <li><a tabindex="-1" href="#">Something else here</a></li>
                </ul>
            </li>
        </ul>
    </li>
    <li>
        <a href="{{ basePath }}/docs/index.html">Docs</a>
    </li>
    <li>
        <a href="{{ basePath }}/tests-runner.html">Specs</a>
    </li>
</ul>
{#
   <div class="em-header navbar navbar-fixed-top">
       <div class="container-fluid">
           <a class="brand etremarin" href="{{ basePath }}">
               <img src="{{basePath}}/img/em-logo.png">
           </a>
       </div>
    </div>           #}
{% endblock %}
{% block ariane %}{% endblock %}
{% block content %}
<div class="container atc-content">

    <section id="maquettes">
        <div class="page-header">
            <h2>Maquettes</h2>
        </div>
        <div class="">
            <ul class="thumbnails">
                {% for layout in layouts.gabarits %}
                <li class="thumbnail atc-thumbnail span3">
                    <h3><a href="/maquettes/{{ layout.key }}.html">
                        <span>{{ layout.title }}</span>
                    </a></h3>
                    <a href="/maquettes/{{ layout.key }}.html" class="ir">
                        <img alt="{{ layout.title }}" src="/maquettes/creas/thumbs/{{ layout.image_name|replace({'@width@': '' }) }}" />
                    </a>
                    <div class="caption">
                        <ul class="nav nav-pills">
                            <li>
                                <a class="dropdown-toggle" rel="tooltip" title="Ouvre les gabarits psd/ c" href="#"
                                   data-toggle="dropdown"
                                   data-target="parent">
                                    <i class="icon-picture"></i>
                                    <b class="caret"></b>
                                </a>
                                <ul id="menu-creas-{{ layout.key }}" class="dropdown-menu nav nav-pills">
                                    <li><i class="icon-chevron-left"></i></li>
                                    {% for view in ['320','768','1024','1280'] %}
                                    <li><a  data-route="#image/{{ layout.key }}/{{view}}"
                                            href="/maquettes/creas/valides/{{ layout.image_name|replace({'@width@': ['_',view]|join }) }}"
                                            target="_blank">{{ view }}px</a></li>
                                    {% endfor %}
                                    <li><i class="icon-chevron-right"></i></li>
                                </ul>
                            </li>

                            <li>
                                <a class="dropdown-toggle" rel="tooltip" title="Ouvre les pages réalisées en target _ blank ou dans des popups" href="#"
                                   data-toggle="dropdown"
                                   data-target="parent">
                                    <i class="icon-fullscreen"></i>
                                    <b class="caret"></b>
                                </a>
                                <ul id="menu-real-{{ layout.key }}" class="dropdown-menu    nav nav-pills">
                                    {% for view in ['mobile','desktop','768x1024','1024x768','1280x800'] %}
                                    <li><a href="#page/{{ layout.key }}/{{ view }}" class=".see-{{ view }}">{{ view }}</a></li>
                                    {% endfor %}
                                </ul>
                            </li>

                        </ul>
                    </div>
                </li>
                {% endfor %}
            </ul>
        </div>
    </section>

    <section id="carousel">
        <div class="page-header">
            <h2>Gabarits (psd)</h2>
        </div>
        <div class="row-fluid">
            <div class="span12 well">
                <div id="layoutsCarousel" class="carousel slide">
                    <!-- Carousel items -->
                    <div class="carousel-inner">
                        {% for layout in layouts.gabarits %}
                        <div class="item{% if loop.first %} active{% endif %}">
                            <img class="" src="/maquettes/creas/thumbs/{{ layout.image_name|replace({'@width@': '_'.view }) }}" alt="" />
                            <div class="carousel-caption">
                                <h3>{{ layout.key }}</h3>
                                <div class="btn-toolbar pull-right">
                                    {% for view in ['320','768','1024','1280'] %}
                                    <a
                                            href="#creas/{{ view }}" class="btn"

                                            data-image="/maquettes/creas/thumbs/{{ layout.image_name|replace({'@width@': '_'.view }) }}">{{ view }}px</a>

                                    {% endfor %}
                                </div>
                                <p>{{ layout.description }}</p>
                            </div>
                        </div>
                        {% endfor %}
                    </div>
                    <!-- Carousel nav -->
                    <a class="carousel-control left" href="#layoutsCarousel" data-slide="prev">&lsaquo;</a>
                    <a class="carousel-control right" href="#layoutsCarousel" data-slide="next">&rsaquo;</a>
                </div>
            </div>
        </div>
    </section>

    <section id="responsive">
        <header>
            <div class="close"><a href="#">×</a></div>
            <div id="size" style="min-width: 0px;">320x480</div>
            <div class="keyboard"><a href="#">I</a></div>
            <div class="cssrefresh"><a href="#" class="">I</a></div>
            <div id="devices"><a href="#" class="tablet-portrait"><span>Tablet Portrait</span></a><a href="#"
                                                                                                     class="tablet-landscape"><span>Tablet Landscape</span></a><a
                    href="#" class="smartphone-landscape"><span>iPhone Landscape</span></a><a href="#"
                                                                                              class="smartphone-portrait active"><span>iPhone Portrait</span></a><a
                    href="#" class="auto"><span>Auto</span></a></div>
        </header>
        <section>
            <div id="wrapper" style="width:320px;height:480px;margin-top:20px;" data-device="smartphonePortrait">
                <iframe src="http://stuffandnonsense.co.uk/projects/320andup/"
                        onload="resbook.changeUrl(this.contentWindow.location,this.contentDocument.title);"></iframe>
                <span class="keyboard-bg"></span></div>
        </section>
    </section>
    <section id="specs">

        <div class="row-fluid">
            <div class="page-header">
                <h2>Rapports de Tests</h2>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span3 well">
                <ul class="nav nav-list">
                    <li class="nav-title">Comportement</li>
                    <li>Jasmine BDD</li>
                    <li class="nav-title">Int&eacute;gration</li>
                    <li>Selenium grid</li>
                    <li>JMeter</li>
                </ul>
            </div>
            <div class="span9 well">
                <div class="">
                    <table>
                        <thead></thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>

    </section>
    <section id="documentation">
        <div class="page-header">
            <h2>Documentation</h2>
        </div>
        <div class="row-fluid">
            <div class="span2 well">

                <ul class="subnav affix">
                    {% for link in page.index %}
                    <li><a href="{{ link.url }}">{{ link.text }}</a></li>
                    {% endfor %}
                </ul>
            </div>
            <div class="span10 well">
                <div class="em-contenu contrib">
                    {{ page.documentation | raw }}
                </div>
            </div>
        </div>
    </section>
</div>
{% endblock %}


{% block footer %}{% endblock %}

{% block templates %}
<script id="modal-template" type="text/template">
    <div class="modal">
        <div class="modal-header"><h3><%= title %></h3></div>
        <div class="modal-body"><%= body %></div>
        <div class="modal-footer"><a class="btn close">Fermer</a></div>
    </div>
    <div class="modal-backdrop"></div>
</script>

<script id="thumbnail-template" type="text/template">
    <li class="span4">
        <div class="thumbnail">
            <img alt="" src="creas/thumbs/{{layout.image_name}}.jpg">
            <div class="caption">
                <h3>Sous Home <a href="#" data-toggle="dropdown" class="btn btn-primary dropdown-toggle pull-right"><span class="icon-caret-down"></span></a></h3>

                <p><a class="btn" href="/maquettes/{{layout.key}}.html">Voir</a><!--<a class="btn" href="#">Action</a></p>-->
            </div>
        </div>
    </li>
</script>
<script id="progressbar-template" type="text/template">
    <div class="progress">
        <div class="bar bar-success" style="width: 35%;"></div>
        <div class="bar bar-warning" style="width: 20%;"></div>
        <div class="bar bar-danger" style="width: 10%;"></div>
    </div>
</script>      {% endblock %}
{% block bottom_js %}{% endblock %}

<script data-main="js/engine" src="{{ basePath }}/js/require.js"></script>


</body>
</html>